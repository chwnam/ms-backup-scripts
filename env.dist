# 필수. 원드라이브에 저장할 설정 경로.
CONF_REMOTE_PATH=onedrive:Conf_Backup

# 선택. 한줄에 하나씩 백업하고 싶은 항목을 적는다.
CONF_TARGETS="
a
b
c
d
"

# 촤대 수용할 설정 백업 갯수. 비워두면 5.
CONF_CAPACITY=

# 필수. 원드라이브에 저장할 WP 경로.
WP_REMOTE_PATH=onedrive:WP_Backup

# 최대 수용할 WP 백업 갯수. 비워두면 5.
WP_REMOTE_CAPACITY=

# 필수. 백업할 WP 루트 경로. wp-config.php 파일이 있는 거기.
WP_ROOT=

